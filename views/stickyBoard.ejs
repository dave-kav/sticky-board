<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Sticky Wars</title>

  <!-- Bootstrap core CSS -->
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link href="styles/all.css" rel="stylesheet">
  <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script src="js/bootstrap.bundle.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>
  <script src="js/sticky.js"></script>
</head>

<body id="page-top">

  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
    <div class="container">
      <a class="navbar-brand js-scroll-trigger" href="#page-top">Sticky Wars</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarResponsive">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <div class="dropdown">
              <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Users
                <span class="caret"></span></button>
                <ul class="dropdown-menu">
                  <li><a>Dave</a></li>
                  <li><a>Andy</a></li>
                </ul>
              </div>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="#services">Timer</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>

    <br>

    <!-- Sticky Section -->
    <div id="addon" class="container">
      <div class="row">
        <div id="dev-col" class="col-lg-4 sticky-col scroll-area">
            <h2>DEV</h2>
            <div class="stackAll stackDEV invisible hide-init"></div>
        </div>
        <div id="auto-col" class="col-lg-4 sticky-col scroll-area">
            <h2>auto</h2>
            <div class="stackAll stackAUTO invisible hide-init"></div>
        </div>
        <div id="qa-col" class="col-lg-4 sticky-col scroll-area">
          <h2>QA</h2>
          <div class="stackAll stackQA invisible hide-init"></div>
        </div>
      </div>
    </div>

    <!-- Bin -->
    <div id="trash" ><i class="fa fa-trash fa-5x"></i></div>

    <!-- Input section -->
    <div id="footer" class="">
      <form id='sticky-form' class="form-inline container">
        <div class="form-group">
          <select id="task_type">
            <option value="dev">Dev</option>
            <option value="auto">AUTO</option>
            <option value="qa">QA</option>
          </select>  
        </div>
        <div class="form-group">
          <input type="text" class="form-control mb-6 mr-sm-2 mb-sm-0" id="inlineFormInputName2" placeholder="Task">
        </div>
        <div>
          <button id="button-1" type="submit" class="btn btn-primary">Submit</button>
        </div>
      </form>
    </div>
  </body>
  </html>